	<a class="right carousel-control" data-slide="next" href="#myCarousel">›</a>
</div>

<!-- Events -->
<div class="tabbable-panel">
	<div class="tabbable-line">
		<ul class="nav nav-tabs ">
			<li id="monday-mark">
				<a href="#monday" data-toggle="tab">Lunes</a>
			</li>
			<li id="tuesday-mark">
				<a href="#tuesday" data-toggle="tab">Martes</a>
			</li>
			<li id="wednesday-mark">
				<a href="#wednesday" data-toggle="tab">Miercoles</a>
			</li>
			<li id="thursday-mark">
				<a href="#thursday" data-toggle="tab">Jueves</a>
			</li>
			<li id="friday-mark">
				<a href="#friday" data-toggle="tab">Viernes</a>
			</li>
			<li id="saturday-mark">
				<a href="#saturday" data-toggle="tab">Sabado</a>
			</li>
			<li id="sunday-mark">
				<a href="#sunday" data-toggle="tab">Domingo</a>
			</li>
		</ul>
		<div class="tab-content">
			<div class="tab-pane" id="monday">
				<% @events.each do |e| %>
					<div id="events-pic" class="pic">
						<img src="<%= e.club_logo.url %>" class="pic-image" alt="Pic">
						<span id="pic-caption-events" class="pic-caption left-to-right">
						  <h1 class="pic-title"><%= e.name %></h1>
						  <p class="pic-subtitle"><%= e.address %></p>
						</span>
					</div>
				<% end %>
			</div>
			<div class="tab-pane" id="tuesday">
				<% @events.each do |e| %>
					<div id="events-pic" class="pic">
						<img src="<%= e.club_logo.url %>" class="pic-image" alt="Pic">
						<span id="pic-caption-events" class="pic-caption left-to-right">
						  <h1 class="pic-title"><%= e.name %></h1>
						  <p class="pic-subtitle"><%= e.address %></p>
						</span>
					</div>
				<% end %>
			</div>
			<div class="tab-pane" id="wednesday">
				<% @events.each do |e| %>
					<div id="events-pic" class="pic">
						<img src="<%= e.club_logo.url %>" class="pic-image" alt="Pic">
						<span id="pic-caption-events" class="pic-caption left-to-right">
						  <h1 class="pic-title"><%= e.name %></h1>
						  <p class="pic-subtitle"><%= e.address %></p>
						</span>
					</div>
				<% end %>
			</div>
			<div class="tab-pane" id="thursday">
				<% @events.each do |e| %>
					<div id="events-pic" class="pic">
						<img src="<%= e.club_logo.url %>" class="pic-image" alt="Pic">
						<span id="pic-caption-events" class="pic-caption left-to-right">
						  <h1 class="pic-title"><%= e.name %></h1>
						  <p class="pic-subtitle"><%= e.address %></p>
						</span>
					</div>
				<% end %>
			</div>
			<div class="tab-pane" id="friday">
				<% @events.each do |e| %>
					<div id="events-pic" class="pic">
						<img src="<%= e.club_logo.url %>" class="pic-image" alt="Pic">
						<span id="pic-caption-events" class="pic-caption left-to-right">
						  <h1 class="pic-title"><%= e.name %></h1>
						  <p class="pic-subtitle"><%= e.address %></p>
						</span>
					</div>
				<% end %>
			</div>
			<div class="tab-pane" id="saturday">
				<% @events.each do |e| %>
					<div id="events-pic" class="pic">
						<img src="<%= e.club_logo.url %>" class="pic-image" alt="Pic">
						<span id="pic-caption-events" class="pic-caption left-to-right">
						  <h1 class="pic-title"><%= e.name %></h1>
						  <p class="pic-subtitle"><%= e.address %></p>
						  <%= link_to 'Ver evento', event_path(e), class: 'btn btn-default btn-sm' %>
						</span>
					</div>
				<% end %>
			</div>
			<div class="tab-pane" id="sunday">
				<% @events.each do |e| %>
					<div id="events-pic" class="pic">
						<img src="<%= e.club_logo.url %>" class="pic-image" alt="Pic">
